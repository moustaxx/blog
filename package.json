{
	"name": "blog",
	"description": "Personal blog build with Gatsby and Netlify CMS",
	"private": true,
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"start": "nodemon --exec \"gatsby develop\"",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
		"lint:fix": "yarn lint --fix"
	},
	"nodemonConfig": {
		"ignore": [
			".cache/*",
			"public/*",
			"typings/*",
			"src/*",
			".eslintrc.js",
			".gitignore",
			"schema.json"
		],
		"delay": "1500"
	},
	"dependencies": {
		"@material-ui/styles": "^4.4.3",
		"dompurify": "^2.0.3",
		"gatsby": "^2.15.28",
		"gatsby-image": "^2.2.23",
		"gatsby-plugin-extract-schema": "^0.0.5",
		"gatsby-plugin-manifest": "^2.2.20",
		"gatsby-plugin-material-ui": "^2.1.6",
		"gatsby-plugin-netlify": "^2.1.17",
		"gatsby-plugin-netlify-cms": "^4.1.21",
		"gatsby-plugin-offline": "^3.0.11",
		"gatsby-plugin-react-helmet": "^3.1.10",
		"gatsby-plugin-sharp": "^2.2.27",
		"gatsby-plugin-typescript": "^2.1.11",
		"gatsby-remark-copy-linked-files": "^2.1.23",
		"gatsby-remark-images": "^3.1.25",
		"gatsby-remark-relative-images": "^0.2.3",
		"gatsby-source-filesystem": "^2.1.28",
		"gatsby-transformer-remark": "^2.6.26",
		"gatsby-transformer-sharp": "^2.2.19",
		"graphql": "^14.5.8",
		"kebab-case": "^1.0.0",
		"netlify-cms-app": "^2.9.7",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"react-typography": "^0.16.19",
		"typeface-roboto": "^0.0.75",
		"typeface-roboto-slab": "^0.0.72",
		"typography": "^0.16.19",
		"urql": "^1.6.1"
	},
	"devDependencies": {
		"@types/dompurify": "^0.0.33",
		"@types/react-helmet": "^5.0.10",
		"@types/typography": "^0.16.3",
		"@typescript-eslint/eslint-plugin": "2.3.3",
		"@typescript-eslint/parser": "2.3.3",
		"eslint": "^6.4.0",
		"eslint-config-airbnb-typescript": "^5.0.0",
		"eslint-plugin-graphql": "^3.1.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-react-hooks": "^2.0.1",
		"nodemon": "^1.19.2",
		"typescript": "^3.6.3"
	}
}
